{
    "name": "Submission Notifier",
    "description": "This extension notifies the user whenever a solution is submitted in a competitive programing platform",
    "version": "1.0",
    "manifest_version": 3,
    "action":{
        "default_popup": "popup.html",
        "default_icon": "images/farmer.png"
    },
    "commands":{
        "required_actions":{
            "suggested_key":{
                "default":"Ctrl+Shift+F",
                "mac":"MacCtrl+Shift+F"
            },
            "description": "xyz"
        }
    }
}